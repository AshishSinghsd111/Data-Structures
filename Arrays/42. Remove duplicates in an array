//this is a gfg potd for 28th october 2024.




//java solution

class Solution {
    ArrayList<Integer> removeDuplicate(int arr[]) {

        ArrayList<Integer> ans=new ArrayList<>();
        for(int i=0;i<arr.length;i++)
                if(!ans.contains(arr[i]))
                        ans.add(arr[i]);
        return ans;
    }
